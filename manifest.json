{
    "name": "Hello TEST",
    "description": "Base Level Extension",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "hello.html",
        "default_icon": "hello_extensions.png"
    },
    "permissions": [
        "identity",
        "tabs",
        "scripting"
    ],
    "host_permissions": [
        "https://xrvicqszlafncvfmqydp.supabase.co/*",
        "<all_urls>"
    ],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"],
            "css": ["style.css"],
            "all_frames": true
        }
    ]
  }